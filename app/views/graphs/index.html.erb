<script src="/highcharts/js/highcharts.js"></script>
<h1>レポートグラフ</h1>
<div class="row-fluid">
	<div class="span2">
		<%= render 'sidebar' %>
    </div>
    <div class="span5">
    	<%= high_chart("day_created", @day_created) %><br>
    	<%= high_chart("rank_stacking", @rank_stacking) %>
    </div>
    <div class="span5">
    	<%= high_chart("week_created", @week_created) %><br>
    	<%= high_chart("closing", @closing) %>
    </div>

</div>
