- content_for :head do
  script src=="http://maps.google.com/maps/api/js?sensor=false" type="text/javascript"

javascript:
  var map_canvas;
  var markersArray = [];
  function init() {
    var myLatlng = new google.maps.LatLng(35.424824,139.42438);
    var mapOptions = {
      zoom: 8,
      center: myLatlng,
      mapTypeId: google.maps.MapTypeId.ROADMAP
    }
    map_canvas = new google.maps.Map(document.getElementById("map_canvas"), mapOptions);
  }
  google.maps.event.addDomListener(window, 'load', init);

.row-fluid
  .span2
    = render(:partial => "search_form", locals:{ options:   {url: {action: "map"},method: "get" , html: {id: "map_search_form"} }}) 
  .span10
    div style='width: 800px;'
      div#map_canvas style='width: 100%;height: 800px;'
